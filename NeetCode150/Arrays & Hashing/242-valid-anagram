class Solution(object):
    def isAnagram(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """

        if len(s) != len(t):
            return False

        countS, countT = {}, {}

        for n in range(len(s)):
            countS[s[i]] = 1 + countS.get(s[i], 0)
            
